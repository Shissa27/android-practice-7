<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation1"
    app:startDestination="@layout/fragment_first">

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.practica_1.ui.Fragment.FirstFragment"
        android:label="Main"
        tools:layout="@layout/fragment_first">
        <action
            android:id="@+id/action_firstFragment_to_secondFragment3"
            app:destination="@id/secondFragment" />
        <action
            android:id="@+id/action_firstFragment_to_thirdFragment"
            app:destination="@id/thirdFragment" />
    </fragment>

    <fragment
        android:id="@+id/secondFragment"
        android:name="com.example.practica_1.ui.Fragment.SecondFragment"
        android:label="Second"
        tools:layout="@layout/fragment_second"/>
    <fragment
        android:id="@+id/thirdFragment"
        android:name="com.example.practica_1.ui.Fragment.ThirdFragment"
        android:label="third"
        tools:layout="@layout/third_fragment"/>
</navigation>